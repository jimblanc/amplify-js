{
	"name": "@aws-amplify/core",
	"version": "6.1.1",
	"description": "Core category of aws-amplify",
	"main": "./lib/index.js",
	"module": "./lib-esm/index.js",
	"typings": "./lib-esm/index.d.ts",
	"publishConfig": {
		"access": "public"
	},
	"sideEffects": [
		"./lib/I18n/index.js",
		"./lib/Credentials.js",
		"./lib-esm/I18n/index.js",
		"./lib-esm/Credentials.js",
		"./lib/Cache/index.js",
		"./lib-esm/Cache/index.js"
	],
	"scripts": {
		"test": "npm run lint && jest -w 1 --coverage",
		"test:size": "size-limit",
		"build-with-test": "npm test && npm run build",
		"build:cjs": "rimraf lib && tsc -m commonjs --outDir lib && webpack && webpack --config ./webpack.config.dev.js",
		"build:esm": "rimraf lib-esm && tsc -m esnext --outDir lib-esm",
		"build:cjs:watch": "rimraf lib && tsc -m commonjs --outDir lib --watch",
		"build:esm:watch": "rimraf lib-esm && tsc -m esnext --outDir lib-esm --watch",
		"build": "npm run clean && npm run generate-version && npm run build:esm && npm run build:cjs",
		"generate-version": "genversion src/Platform/version.ts --es6 --semi --source ../aws-amplify",
		"clean": "npm run clean:size && rimraf lib-esm lib dist",
		"clean:size": "rimraf dual-publish-tmp tmp*",
		"format": "echo \"Not implemented\"",
		"lint": "tslint 'src/**/*.ts' && npm run ts-coverage",
		"prepublishOnly": "npm run build",
		"ts-coverage": "typescript-coverage-report -p ./tsconfig.json -t 92.36"
	},
	"react-native": {
		"./lib/index": "./lib-esm/index.js"
	},
	"exports": {
		".": {
			"types": "./lib-esm/index.d.ts",
			"import": "./lib-esm/index.js",
			"require": "./lib/index.js"
		},
		"./server": {
			"types": "./lib-esm/server.d.ts",
			"import": "./lib-esm/server.js",
			"require": "./lib/server.js"
		},
		"./internals/adapter-core": {
			"types": "./lib-esm/adapterCore/index.d.ts",
			"import": "./lib-esm/adapterCore/index.js",
			"require": "./lib/adapterCore/index.js"
		},
		"./internals/aws-client-utils": {
			"types": "./lib-esm/clients/index.d.ts",
			"import": "./lib-esm/clients/index.js",
			"require": "./lib/clients/index.js"
		},
		"./internals/aws-client-utils/composers": {
			"types": "./lib-esm/clients/internal/index.d.ts",
			"import": "./lib-esm/clients/internal/index.js",
			"require": "./lib/clients/internal/index.js"
		},
		"./internals/aws-clients/cognitoIdentity": {
			"types": "./lib-esm/awsClients/cognitoIdentity/index.d.ts",
			"import": "./lib-esm/awsClients/cognitoIdentity/index.js",
			"require": "./lib/awsClients/cognitoIdentity/index.js"
		},
		"./internals/aws-clients/pinpoint": {
			"types": "./lib-esm/awsClients/pinpoint/index.d.ts",
			"import": "./lib-esm/awsClients/pinpoint/index.js",
			"require": "./lib/awsClients/pinpoint/index.js"
		},
		"./internals/providers/pinpoint": {
			"types": "./lib-esm/providers/pinpoint/index.d.ts",
			"import": "./lib-esm/providers/pinpoint/index.js",
			"require": "./lib/providers/pinpoint/index.js"
		},
		"./internals/utils": {
			"types": "./lib-esm/libraryUtils.d.ts",
			"import": "./lib-esm/libraryUtils.js",
			"require": "./lib/libraryUtils.js"
		},
		"./package.json": "./package.json"
	},
	"typesVersions": {
		">=4.2": {
			"server": [
				"./lib-esm/server.d.ts"
			],
			"internals/adapter-core": [
				"./lib-esm/adapterCore/index.d.ts"
			],
			"internals/aws-client-utils": [
				"./lib-esm/clients/index.d.ts"
			],
			"internals/aws-client-utils/composers": [
				"./lib-esm/clients/internal/index.d.ts"
			],
			"internals/aws-clients/cognitoIdentity": [
				"./lib-esm/awsClients/cognitoIdentity/index.d.ts"
			],
			"internals/aws-clients/pinpoint": [
				"./lib-esm/awsClients/pinpoint/index.d.ts"
			],
			"internals/providers/pinpoint": [
				"./lib-esm/providers/pinpoint/index.d.ts"
			],
			"internals/utils": [
				"./lib-esm/libraryUtils.d.ts"
			]
		}
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/aws-amplify/amplify-js.git"
	},
	"author": "Amazon Web Services",
	"license": "Apache-2.0",
	"bugs": {
		"url": "https://github.com/aws/aws-amplify/issues"
	},
	"homepage": "https://aws-amplify.github.io/",
	"files": [
		"lib",
		"lib-esm",
		"src",
		"typings.d.ts",
		"internals",
		"server"
	],
	"dependencies": {
		"@aws-crypto/sha256-js": "5.2.0",
		"@aws-sdk/types": "3.398.0",
		"@smithy/util-hex-encoding": "2.0.0",
		"@types/uuid": "^9.0.0",
		"js-cookie": "^3.0.5",
		"rxjs": "^7.8.1",
		"tslib": "^2.5.0",
		"uuid": "^9.0.0"
	},
	"devDependencies": {
		"@aws-amplify/react-native": "1.1.0",
		"@types/js-cookie": "3.0.2",
		"genversion": "^2.2.0",
		"typescript": "5.0.2"
	},
	"size-limit": [
		{
			"name": "Core (Hub)",
			"path": "./lib-esm/index.js",
			"import": "{ Hub }",
			"limit": "1.43 kB"
		},
		{
			"name": "Core (I18n)",
			"path": "./lib-esm/index.js",
			"import": "{ I18n }",
			"limit": "1.46 kB"
		},
		{
			"name": "Custom clients (fetch handler)",
			"path": "./lib-esm/clients/handlers/fetch.js",
			"import": "{ fetchTransferHandler }",
			"limit": "500 B"
		},
		{
			"name": "Custom clients (unauthenticated handler)",
			"path": "./lib-esm/clients/handlers/unauthenticated.js",
			"import": "{ unauthenticatedHandler }",
			"limit": "1 kB"
		},
		{
			"name": "Custom clients (request signer)",
			"path": "./lib-esm/clients/middleware/signing/signer/signatureV4/index.js",
			"import": "{ signRequest }",
			"limit": "3.53 kB"
		},
		{
			"name": "Custom clients (url presigner)",
			"path": "./lib-esm/clients/middleware/signing/signer/signatureV4/index.js",
			"import": "{ presignUrl }",
			"limit": "3.58 kB"
		},
		{
			"name": "Cache (default browser storage)",
			"path": "./lib-esm/index.js",
			"import": "{ Cache }",
			"limit": "3.3 kB"
		}
	],
	"jest": {
		"globals": {
			"ts-jest": {
				"diagnostics": false,
				"tsConfig": false
			}
		},
		"transform": {
			"^.+\\.(js|jsx|ts|tsx)$": "ts-jest"
		},
		"testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
		"testPathIgnorePatterns": [
			"/testUtils/"
		],
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"json",
			"jsx"
		],
		"setupFiles": [
			"./__mocks__/SessionStorage.js",
			"./__mocks__/LocalStorage.js"
		],
		"testEnvironment": "jsdom",
		"coverageThreshold": {
			"global": {
				"branches": 0,
				"functions": 0,
				"lines": 0,
				"statements": 0
			}
		},
		"testURL": "http://localhost/",
		"coveragePathIgnorePatterns": [
			"/node_modules/",
			"dist",
			"lib",
			"lib-esm"
		]
	}
}
